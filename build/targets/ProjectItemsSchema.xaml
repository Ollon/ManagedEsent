<ProjectSchemaDefinitions xmlns="http://schemas.microsoft.com/build/2009/properties">

    <ContentType
        Name="Text"
        DisplayName="Text file"
        ItemType="Content" />

    <ContentType
        Name="XML"
        DisplayName="Xml file"
        ItemType="Content" />

    <ContentType
        Name="HTML"
        DisplayName="Html file"
        ItemType="Content" />

    <ContentType
        Name="CSS"
        DisplayName="Cascading style sheet"
        ItemType="Content" />

    <ContentType
        Name="Json"
        DisplayName="Json file"
        ItemType="Content" />

    <ContentType
        Name="Font"
        DisplayName="Font file"
        ItemType="Content" />

    <ContentType
        Name="Media"
        DisplayName="Media file"
        ItemType="Content" />

    <ContentType
        Name="Image"
        DisplayName="Image file"
        ItemType="Content" />

    <ContentType
        Name="PropertyScript"
        DisplayName="MSBuild Property Script"
        ItemType="Content" />

    <ContentType
        Name="TargetsScript"
        DisplayName="MSBuild Targets Script"
        ItemType="Content" />

    <ContentType
        Name="ProjectScript"
        DisplayName="MSBuild Project Script"
        ItemType="Content" />


    <ContentType
        Name="VSTemplate"
        DisplayName="Project/Item Template (.vstemplate) File"
        ItemType="VSTemplate">
        <NameValuePair Name="DefaultMetadata_SubType" Value="Designer" />
    </ContentType>

    <ContentType
        Name="VSCTCompile"
        DisplayName="Visual Studio Command Table File"
        ItemType="VSCTCompile">
        <NameValuePair Name="DefaultMetadata_ResourceName" Value="Menus.ctmenu" />
        <NameValuePair Name="DefaultMetadata_SubType" Value="Designer" />
    </ContentType>

    <ContentType
        Name="EmbeddedResource"
        DisplayName="Embedded resource"
        ItemType="EmbeddedResource">
        <NameValuePair Name="DefaultMetadata_Generator" Value="ResXFileCodeGenerator" />
    </ContentType>

    <ContentType
        Name="XamlPropertyRule"
        DisplayName="XAML Property Rule"
        ItemType="XamlPropertyRule">
        <NameValuePair Name="DependentExtensions" Value=".xaml.cs" />
        <NameValuePair Name="DefaultMetadata_SubType" Value="Designer" />
        <NameValuePair Name="DefaultMetadata_Generator" Value="MSBuild:GenerateRuleSourceFromXaml" />
    </ContentType>

    <ContentType
        Name="AppXaml"
        DisplayName="XAML Application Definition"
        ItemType="ApplicationDefinition">
        <NameValuePair Name="DependentExtensions" Value="xaml.cs" />
        <NameValuePair Name="DefaultMetadata_SubType" Value="Designer" />
        <NameValuePair Name="DefaultMetadata_Generator" Value="MSBuild:Compile" />
    </ContentType>

    <ContentType
        Name="PageXaml"
        DisplayName="XAML Page"
        ItemType="Page">
        <NameValuePair Name="DependentExtensions" Value=".xaml.cs" />
        <NameValuePair Name="DefaultMetadata_SubType" Value="Designer" />
        <NameValuePair Name="DefaultMetadata_Generator" Value="MSBuild:Compile" />
    </ContentType>

    <ContentType
        Name="LinqToXsdSchema"
        DisplayName="Linq To Xsd Schema (.xsd)"
        ItemType="LinqToXsdSchema" />


    <ItemType Name="LinqToXsdSchema" DisplayName="Linq to Xsd Schema" />
    <ItemType Name="Page" DisplayName="XAML Page" />
    <ItemType Name="ApplicationDefinition" DisplayName="ApplicationDefinition" />
    <ItemType Name="None" DisplayName="None" />
    <ItemType Name="VSTemplate" DisplayName="Project/Item Template (.vstemplate) File" />
    <ItemType Name="None" DisplayName="None" />
    <ItemType Name="Content" DisplayName="Content" />
    <ItemType Name="EmbeddedResource" DisplayName="Embedded resource" />
    <ItemType Name="XamlPropertyRule" DisplayName="XAML Property Rule" />




    <FileExtension Name=".props" ContentType="Content" />
    <FileExtension Name=".targets" ContentType="Content" />
    <FileExtension Name=".proj" ContentType="Content" />

    <FileExtension Name=".asax" ContentType="Asax" />
    <FileExtension Name=".asmx" ContentType="HTML" />
    <FileExtension Name=".asp" ContentType="AspPage" />
    <FileExtension Name=".txt" ContentType="Text" />
    <FileExtension Name=".resx" ContentType="EmbeddedResource" />
    <FileExtension Name=".html" ContentType="HTML" />
    <FileExtension Name=".htm" ContentType="HTML" />
    <FileExtension Name=".css" ContentType="CSS" />
    <FileExtension Name=".cur" ContentType="CUR" />
    <FileExtension Name=".vbS" ContentType="Script" />
    <FileExtension Name=".jS" ContentType="Script" />
    <FileExtension Name=".xml" ContentType="XML" />
    <FileExtension Name=".mht" ContentType="MHT" />
    <FileExtension Name=".manifest" ContentType="Manifest" />
    <FileExtension Name=".rdlc" ContentType="RDLC" />
    <FileExtension Name=".cd" ContentType="ClassDiagram" />
    <FileExtension Name=".licenses" ContentType="Licenses" />
    <FileExtension Name=".json" ContentType="Json" />

